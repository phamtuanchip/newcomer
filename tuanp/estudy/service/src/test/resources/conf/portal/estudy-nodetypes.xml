<?xml version="1.0" encoding="ISO-8859-1"?>
<nodeTypes xmlns:nt="ht exo://www.jcp.org/jcr/nt/1.0" 
           xmlns:mix="ht exo://www.jcp.org/jcr/mix/1.0" 
           xmlns:jcr="ht exo://www.jcp.org/jcr/1.0">

  <nodeType name="exo:category" isMixin="false" hasOrderableChildNodes="false">
    <supertypes>
      <supertype>nt:base</supertype>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      
      <propertyDefinition name="exo:categoryname" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
     
    </propertyDefinitions>
    
  </nodeType>


  <nodeType name="exo:question" isMixin="false" hasOrderableChildNodes="false">
    <supertypes>
      <supertype>nt:base</supertype>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      
      <propertyDefinition name="exo:title" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

       <propertyDefinition name="exo:answer" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="true">
        <valueConstraints/>
      </propertyDefinition>
     
    </propertyDefinitions>
    
  </nodeType>

  <nodeType name="exo:testing" isMixin="false" hasOrderableChildNodes="false">
    <supertypes>
      <supertype>nt:base</supertype>
      <supertype>mix:referenceable</supertype>
    </supertypes>
    <propertyDefinitions>
      
      <propertyDefinition name="exo:time" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

       <propertyDefinition name="exo:point" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

      <!-- reference to  exo:question -->
      <propertyDefinition name="exo:rfquestion" requiredType="Reference" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
     
    </propertyDefinitions>
    
  </nodeType>

  <nodeType name="exo:session" isMixin="false" hasOrderableChildNodes="false">
    <supertypes>
      <supertype>nt:base</supertype>
    </supertypes>
    <propertyDefinitions>
      <propertyDefinition name="exo:title" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:description" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:rflink" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>
      <propertyDefinition name="exo:vlink" requiredType="String" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
        <valueConstraints/>
      </propertyDefinition>

    </propertyDefinitions>
    <childNodeDefinitions>
      <childNodeDefinition name="exo:content" defaultprimaryType="nt:file" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" sameNameSiblings="false">
        <requiredPrimaryTypes>
          <requiredPrimaryType>nt:file</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>

      <childNodeDefinition name="exo:video" defaultprimaryType="nt:file" autoCreated="false" mandatory="false" onParentVersion="COPY" protected="false" sameNameSiblings="false">
        <requiredPrimaryTypes>
          <requiredPrimaryType>nt:file</requiredPrimaryType>
        </requiredPrimaryTypes>
      </childNodeDefinition>
    </childNodeDefinitions>
  </nodeType>
   
</nodeTypes>
